/* 共用样式 */
html,body, div, ul, ol, dl, dt, dd, li, dl, h1, h2, h3, h4{margin:0;padding:0;font-style:normal;} 
ol, ul ,li{list-style: none;} 
a{color:#333;text-decoration: none;-webkit-touch-callout: none;-webkit-user-select: none;}
a,input,select{-webkit-tap-highlight-color: transparent;-webkit-appearance: none;-moz-appearance: none;-webkit-border-radius: 0;}
:focus{outline: none}
table{border-collapse:collapse;border-spacing:0}
.clearfix{clear:both;} 
.mui-btn:enabled:active, 
.mui-btn.mui-active:enabled{
	background: transparent;
}
p{
	color: #333333;
	font-size: 0.24rem;
}
.max-width{
	max-width: 100%;
	max-height: 100%;
	}
.bg-white{
	background: #FFFFFF;
}
input::-webkit-input-placeholder{
	color: #BBBBBB;
}
.bOverhidden{
	overflow: hidden;
}
.no-date{
	text-align: center;
	color: #888888;
	margin: 1rem auto;
}
/* 顶部 */
.mui-bar{
	background: #33323E;
	box-shadow: none;
}
.mui-bar .add-icon{
	font-size: 0.52rem;
    font-weight: bold;
    padding-top: 8px;
}
.mui-bar .icon-text{
	padding-top: 15px;
	font-size: 0.3rem;
}
.mui-bar .mui-title,
.mui-bar .mui-icon{
	color: #FFFFFF;
}
/* 企业列表 */
.location-title{
	font-size: 0.28rem;
	padding: 0.2rem 0.2rem 0.2rem 1rem;
	background: url(../ani-image/location-tip.png) no-repeat 0.65rem 0.23rem #FFFFFF;
	background-size: 0.24rem 0.32rem;
}
.company-search-cont{
	background: #FFFFFF;
	padding: 0.15rem 0.2rem;
	height: 50px;
	position: relative;
}
.company-search-cont .mui-search{
	margin-right: 80px;
}
.company-search-cont input[type='search']{
	margin-bottom: 0;
	font-size: 0.24rem;
	background: #FFFFFF;
	border-radius: 5px;
	border: 1px solid #E3E3E3;
}
.company-search-cont .mui-search:before{
    margin-top: -10px;
}
.company-search-cont .mui-btn{
	position: absolute;
    right: 0.2rem;
    top: 0.15rem;
	color:rgba(71,148,244,1);
	border-radius: 5px;
	padding: 6px 20px;
	border:1px solid rgba(71, 148, 244, 1)
}
.company-search-cont .mui-btn.active{
	color: #888888;
	border:1px solid #888888;
}
.company-search-cont .mui-placeholder{
	font-size: 0.26rem;
}
.company-panel{
	margin: 0.2rem;
	padding: 0.15rem;
	background:rgba(255,255,255,1);
	border:1px solid rgba(210, 209, 219, 0.6);
	border-radius:8px;
}
.company-panel .title{
	position: relative;
	font-size: 0.28rem;
	line-height: 0.6rem;
	padding-left: 0.2rem;
}
.company-panel .mapGo{
	position: absolute;
	z-index: 1;
	right: 10px;
	width: 0.24rem;
}
.company-panel .company-detail{
	font-size: 0.24rem;
	color: #6A6882;
	line-height: 0.46rem;
	padding: 0 10px;
}
.company-panel .company-detail p{
	color: #6A6882;
}
.company-panel .company-detail p.mui-pull-right{
	width: 50%;
}
.company-panel .btn-list{
	clear: both;
	border-top: 1px solid rgba(210,209,219,1);;
}
.company-panel .list-btn{
	text-align: center;
	font-size: 0.26rem;
	color: #4989F2;
	padding-top: 0.15rem;;
}
/* 消息列表 */
.message-content .mui-table-view-cell{
	font-size: 0.24rem;
	margin-bottom: 10px;
	border-bottom: 1px solid #D2D1DB;
}
.message-content .mui-table-view-cell:after{
	height: 0;
}
.message-content .mui-navigate-right{
	padding-right: 35px !important;
	background: #FFFFFF;
}
.message-content .mui-table-view{
	background: transparent;
}


/* 添加企业 */
.company-add .mui-input-group{
	margin-bottom: 10px;
}
.company-add .mui-input-row label{
	font-size: 0.24rem;
	width: 25%;
}
.company-add .mui-input-row label ~ input,
.company-add .mui-input-row select{
	font-size: 0.24rem;
	margin-bottom: 0;
	width: 75%;
}
.company-add-btn,
.log-add-btn{
	width:96%;
	margin: 20px 2%;
	color: #FFFFFF;
	font-size: 0.28rem;
	padding: 10px 0;
	background:linear-gradient(90deg,rgba(73,134,242,1),rgba(68,174,247,1));
	border-radius:5px;
	border: none;
}	
.company-add .name-input{
	padding-right: 1.2rem;
}
.company-add .company-name .mui-icon-clear{
	right: 1.2rem !important;
}
.company-add .searchBtn{
	position: absolute;
    right: 0.15rem;
    font-size: 0.26rem;
    color: #4989F2;
    border: 1px solid rgba(72, 140, 243, 1);
    border-radius: 5px;
    padding: 0.05rem 0.15rem;
    margin: 0.08rem;
}
.company-add .modify-tip{
	background: #FFFFFF url(../ani-image/location-tip.png) no-repeat 98%;
	background-size: 0.3rem auto;
}
.company-add .com-add-itp{
	color: #6A6882;
	margin: 0.4rem 0 0 10px;
}
.company-add .com-add-itp span{
	color: #00CC9B;
	margin-right: 5px;
}
.company-add .com-popover{
	line-height: 40px;
    font-size: 0.26rem;
    width: 100%;
    display: block;
	color: #BDBDC6;
}
.modify-mask{
	width: 100%;
	left: 0 !important;
	top: 44px !important;
}
.modify-mask .mui-search{
	margin: 0.2rem 0.2rem 0 0.2rem;
}
.modify-mask .mui-table-view-cell{
	padding: 10px 36px;
	font-size: 0.26rem;
	color: #6A6882;
}
.modify-mask .mui-popover-arrow{
	display: none;
}
/* 日志列表 */
.log-list .mui-table-view-cell{
	font-size: 0.24rem;
}
.log-list .mui-navigate-right{
	padding-right: 35px !important;
}
/* 日志添加 、展示*/
.log-list-ul{
	background: #FFFFFF;
	padding: 0 10px;
	margin-bottom: 10px;
}
.log-list-ul .log-list-li{
	position: relative;
	padding: 10px 0;
	border-bottom: 1px solid #D2D1DB;
}
.log-list-ul .log-list-li:last-child{
	border-bottom: none;
}
.log-list-ul .title{
	position: absolute;
	margin-bottom: 0;
}
.log-list-ul .desc{
	padding-left: 1.5rem;
	margin-bottom: 0;
}
.mui-radio.mui-left input[type='radio']{
	left: 0;
}
.log-list-ul input[type=radio]:checked:before{
	color: #16D0A4;
}
.log-list-ul .mui-radio{
	display: inline-block;
}
.log-list-ul input[type=radio]:before{
	font-size: 0.32rem;
}
.log-list-ul input[type=radio][disabled]:before{
	opacity: 1;
}
.log-list-ul input[type=radio]{
	top: 2px;
}
.log-list-ul .mui-radio.mui-left label{
	padding: 4px 60px 0 25px;
	font-size: 0.24rem;
}

/* 图片上传 */
.log-img{
	background: #FFFFFF;
    padding: 10px;
    margin-bottom: 10px;
}
.log-img li{
	display: inline-block;
	text-align: center;
	line-height: 1.1rem;
	width: 1.1rem;
	height: 1.1rem;
	margin: 0.1rem;
	border: 1px solid #EEEEEE;
}
.log-img img{
	vertical-align: middle;
}
.log-img .upload-content{
	display: inline;
}
.log-img .upload-content li{
	position: relative;
}
.log-img .upload-btn{
	border: none;
}
.upload-content .detele-icon{
	position: absolute;
	top: -0.16rem;
    right: -0.16rem;
    width: 0.32rem;
}

/* textarea框 */
.opinion{
	padding: 10px;
}
.opinion textarea{
	border: none;
	padding: 0;
	margin-bottom: 0;
	height: 1.36rem;
	font-size: 0.24rem;
}

/* 签字 */
.sign-ul{
	padding: 10px;
}
.Sign{
	border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.Sign .title{
	display: inline-block;
	position: inherit;
	line-height: 0.7rem;
}
.Sign .Sign-cont{
	display: inline-block;
	width: 1.5rem;
	height: 0.7rem;
	vertical-align: middle;
}
.Sign .sign_show{
	max-width: 100px;
	max-height: 30px;
	display: inline-block;
	margin-top: 0.06rem;
}
.check-time{
	padding-top: 10px;
	color: #333333;
	font-size: 0.24rem;
}

/* 签字面板 */
.zIndex{
	z-index: -1 !important;
}
.sing-panel{
	position: fixed;
	bottom: 20%;
	left: 10%;
	width: 80%;
	z-index: 999;
	background: #FFFFFF;
}
.sing-panel .panel-content,
.sing-panel .panel-content canvas{
	height: 7rem !important;
}
.sing-panel .btn{
	width: 33.333333%;
    float: left;
    text-align: center;
    font-size: 0.3rem;
    line-height: 0.65rem;
}
.sing-panel .btn.sure-btn{
	background: #2AC845;
}
.sing-panel .btn.clear-btn{
	background: #f8a072;
}
.sing-panel .btn.cancel-btn{
	background: #15a0fb;
}

/* 地址地选择 */
.row-title{
	height: 40px;
}
.row-title li{
	width: 33.33333333%;
	float: left;
	text-align: center;
	margin-top: 8px;
	font-size: 0.28rem;
	border-right: 1px solid #D2D1DB;
}
.row-title li:last-child{
	border-right: none;
}
.row-content{
	margin-top: 10px;
	width: 33.33333333%;
	float: left;
	font-size: 0.24rem;
	line-height: 0.64rem;
	border-right: 1px solid #D2D1DB;
	overflow-y: scroll;
	background: #FFFFFF;
}
.row-content:last-child{
	border-right: none;
}
.row-content li{
	padding: 0 15px;
}
.row-content li.active{
	color: #479AF4;
}
/* 地图 */
.map-tip{
	word-break: break-all;
	position: relative;
	max-width: 5rem;
	padding: 15px 15px 5px 15px;
	background: #FFFFFF;
	border-radius: 5px;
}
.map-tip .com-name{
	font-size: 0.26rem;
	margin-bottom: 5px;
	font-weight: bold;
}
.map-tip .com-address{
	line-height: 0.4rem;
}
.map-tip .tip{
	position: absolute;
	left: 10px;
	bottom: -15px;
	width: 0.6rem;
}
.daohang{
	position: fixed;
	bottom: 1.2rem;
	background: #007AFF;
	border-radius: 30px;
    left: 42%;
	padding: 0 15px 0 10px;
	height: 0.7rem;
	color: #FFFFFF;
	font-size: 0.28rem;
}
.daohang img{
	padding-right: 5px;
    width: 0.5rem;
	float: left;
	padding-top: 0.08rem;
}
.daohang span{
	line-height: 0.7rem;
}
.reMap{
	position: absolute;
    right: 20px;
    bottom: 50px;
    width: 0.56rem;
	padding: 5px;
	border-radius: 50%;
	background: #FFFFFF;
}