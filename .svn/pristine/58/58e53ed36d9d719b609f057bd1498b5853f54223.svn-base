		<script type="text/javascript">
			mui.previewImage();
			mui.plusReady(function(){
				var uuid = plus.storage.getItem("uuid");
				if(uuid == "" || uuid == null || uuid == "undefined"){
					mui.openWindow({
						id: 'login.html',
						url: '../login.html'
					})
				}
				var self = plus.webview.currentWebview();
				var qylx = self.qylx; // 企业类型
				var l_id = self.l_id; // 日志编号
				var comAddress = self.address;
				var comName = self.compName;
				document.getElementById("qymc").innerHTML = comName;
				document.getElementById("scdz").innerHTML = comAddress;
				logView(qylx, l_id);
			});

			// 详情
			function logView(qylx, l_id){
				var _url1=Constant.getConstant("COMPANY_LOG_DETAIL");
				mui.ajax(_url1,{
					data:{
						"qylx": qylx, //为企业类型
						"l_id": l_id //监督日志的id编号
					},
					dataType:'json',//服务器返回json格式数据
					type:'post',//HTTP请求类型
					timeout:10000,//超时时间设置为10秒；
					success:function(data){
						console.log(data.result);
						var path = data.path;
						console.log("监督日志上传照片的存储路径 " + path);
						var jsonobj = data.jsonobj;
						if(jsonobj.scxkz == "有"){
							$("input:radio[name='radio1'][value='是']").attr("checked",true);
						}
						if(jsonobj.cgsljy == "是"){
							$("input:radio[name='radio2'][value='是']").attr("checked",true);
						}
						if(jsonobj.zgyj == '' || jsonobj.zgyj == null || jsonobj.zgyj == undefined){
							document.getElementById("zgyj").value = "暂无意见";
						}else{
							document.getElementById("zgyj").value = jsonobj.zgyj;
						}
						// 企业是否已看过，进行签名确认：0表示没有看过，没有进行签名确认；1标识已经看过，并进行签名确认
						var checked = jsonobj.checked;
						if(jsonobj.checked == 0){

						}else if(jsonobj.checked == 1){

						}

						var jcsj = jsonobj.jgrSignImg;// 监管人 手机端进行电子签名存储的电子签名图片的名称
						var jcsj = jsonobj.qyfzrSignImg;// 企业负责人 手机端进行电子签名存储的电子签名图片的名称

						var date = new Date(jsonobj.jcsj); // 检查日期
						Y = date.getFullYear() + '-';
						M = (date.getMonth()+1 < 10 ? '0'+(date.getMonth()+1) : date.getMonth()+1) + '-';
						D = date.getDate() + ' ';
						document.getElementById("jcsj").innerHTML = Y+M+D;

						var jcsj = jsonobj.imgname;//不合格项上传照片的名称
						if(data.result == "success"){

						}else if(data.result == "failure"){
							mui.toast("查询失败，请重试");
						}else{
							mui.toast("失败");
						}
					},
					error:function(xhr,type,errorThrown){
						mui.toast("查询失败");
					}
				});
			}



		</script>

