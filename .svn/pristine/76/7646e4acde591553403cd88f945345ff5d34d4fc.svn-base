/* 现场收集-无耳标 */
.collect-ear-list{
	margin: 0 0.2rem;
	background: transparent;
}
.collect-ear-list .mui-table-view-cell{
	margin-bottom: 10px;
    background: #fff;
    border-radius: 5px;
}
.collect-ear-list .mui-slider-handle p{
	margin-bottom: 5px;
	color: #6A6882;
}
.collect-ear-list.mui-table-view:before,
.collect-ear-list.mui-table-view:after,
.collect-ear-list .mui-table-view-cell:after,
.collect-ear-list .mui-table-view:before{
	height: 0;
}
.collect-ear-list .mui-table-view-cell > .mui-slider-left > .mui-btn, 
.collect-ear-list .mui-table-view-cell > .mui-slider-right > .mui-btn{
	left: 1px;
}
.collect-ear-list .conclusion,
.collect-ear-list .weight{
	width: 50%;
	float: left;
}
.popover-cont.popover-no-ear .mui-input-row label{
    width: 35%;
}
.popover-no-ear .mui-input-group input{
	text-align: right;
}
.popover-no-ear .mui-input-row:last-child input{
	padding-right: 40px;
}

.ear-num-row .mui-select{
	width: 30%;
	float: left;
}
.ear-num-row .ear-input{
	width: 40%;
	float: left;
}
.ear-weight-row{
	height: 80px !important;
}
.ear-weight-row .title-label-short,
.ear-img-content .title-label-short{
	width: 100% !important;
}
.ear-weight-row input{
	width: 95% !important;
}
.ear-img-content{
	height: auto !important;
}
.ear-img-content ul{
	margin-left: 5%;
	width: 95% !important;
}
